{
    "name": "@qinetik/anique-icons",
    "version": "1.0.2",
    "description": "Emotion JS support for Solid JS projects.",
    "repository": "git@github.com:qinetik/anique.git",
    "author": "Qinetik",
    "license": "MIT",
    "main": "dist/index.cjs.js",
    "module": "dist/index.es.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**"
    ],
    "scripts": {
        "clean": "node -e \"if(require('fs').existsSync('./dist')) require('fs').rmSync('./dist', { recursive : true })\"",
        "build-es": "vite build -c viteconfigs.es.ts",
        "build-cjs": "vite build -c viteconfigs.cjs.ts",
        "build-types": "tsc",
        "build": "yarn run clean && yarn run build-es && yarn run build-cjs && yarn run build-types"
    },
    "devDependencies": {
        "@types/node": "^15.12.5",
        "typescript": "^5.2.2",
        "vike": "^0.4.142",
        "vite": "^4.4.9",
        "vite-plugin-solid": "^2.7.0"
    },
    "peerDependencies": {
        "solid-js": "^1.7.11"
    }
}
